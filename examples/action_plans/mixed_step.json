{
  "summary": "Read, write, run and emit in one deterministic step",
  "actions": [
    {
      "kind": "read",
      "path": "README.md"
    },
    {
      "kind": "write",
      "path": "tmp/mixed-step.txt",
      "content": "This file was generated by examples/action_plans/mixed_step.json\n"
    },
    {
      "kind": "run",
      "command": "git status"
    },
    {
      "kind": "emit",
      "event_type": "done",
      "message": "Mixed-step example finished",
      "blocking": false,
      "payload": {
        "plan": "mixed_step"
      }
    }
  ]
}
